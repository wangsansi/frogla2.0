.list-content {
  padding: 30rpx;
}
.icon-area-big {
  position: absolute;
  height: 300rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.ticket-add-big {
  width: 160rpx;
  height: 160rpx;
  background-color: #266fe8;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  margin-top: 8rpx;
}
.icon-area-small {
  position: fixed;
  bottom: 50rpx;
  right: 30rpx
}
.ticket-add-small {
  width: 100rpx;
  height: 100rpx;
  background-color: #266fe8;
  border-radius: 50%;
}