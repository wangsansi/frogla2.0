/**app.wxss**/
page {
  --main-color:#0052D9;
}
.container {
  min-height: 100vh;
  width: 750rpx;
  box-sizing: border-box;
  background-color: #f6f6f6;
} 
.ticket-card {
  background-color: #ffffff;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}
view {
  box-sizing: border-box;
}
@font-face {
  font-family: 'iconfont';  /* project id 248193 */
  src: url('//at.alicdn.com/t/font_xhktuadcbq3t0529.ttf') format('truetype')
}

.iconfont {
  font-family:"iconfont" !important;
  font-size: 64rpx;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

